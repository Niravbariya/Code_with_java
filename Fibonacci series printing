Fibonacci series printing 
import java.util.*;
public class Main

{
    
    static void fiboSeries(int n,int first,int second)
    {
      if(n>0)
      {
          System.out.print(first+" ");
          fiboSeries(n-1,second,first+second);
      }
    }
	public static void main(String[] args) {
	    Scanner ip = new Scanner(System.in);
	    int n = ip.nextInt();
	    int first = 0;
	    int second = 1;
	    fiboSeries(n,0,1);
	    
	}
}
		
